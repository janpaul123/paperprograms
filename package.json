{
  "name": "paperprograms",
  "version": "0.0.2",
  "description": "Co-design environment for accessible educational simulations.",
  "author": "Jan Paul Posma <j@npaulpos.ma>, PhET Interactive Simulations <phethelp@colorado.edu>",
  "license": "MIT",
  "scripts": {
    "createdb": "psql -c \"SELECT 1\" paper_programs_development 2>/dev/null 1>/dev/null || createdb paper_programs_development",
    "dev": "npm run createdb && npm run migrate && npm start",
    "dev:docker": "npm run migrate && npm start",
    "dropdb": "dropdb --if-exists paper_programs_development",
    "get-programs": "node scripts/db/get-programs-from-database.js",
    "heroku-postbuild": "webpack",
    "lint": "eslint . --rulesdir ../chipper/eslint/rules",
    "lint-fix": "eslint . --fix",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "start": "node server/entry-server.js",
    "test": "npm run lint"
  },
  "engines": {
    "node": "9.3.x",
    "npm": "5.6.x"
  },
  "cacheDirectories": [
    ".babel-cache"
  ],
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.2",
    "blob-stream": "^0.1.3",
    "bootstrap": "^5.2.3",
    "brfs": "^2.0.2",
    "color-diff": "^1.1.0",
    "copy-webpack-plugin": "^4.5.0",
    "css-loader": "^0.28.10",
    "d3": "^5.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "heroku-ssl-redirect": "0.0.4",
    "json-loader": "^0.5.7",
    "knex": "^0.21.1",
    "lodash": "^4.17.5",
    "morgan": "^1.9.0",
    "nocache": "^2.0.0",
    "node-matrices": "^1.0.0",
    "pdfkit": "^0.10.0",
    "pg": "^8.0.3",
    "randomcolor": "^0.5.3",
    "react": "^16.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^16.2.0",
    "react-monaco-editor": "^0.13.0",
    "shuffle-seed": "^1.1.6",
    "string-replace-webpack-plugin": "^0.1.3",
    "style-loader": "^0.19.1",
    "transform-loader": "^0.2.4",
    "uuid": "^3.2.1",
    "webpack": "^5.75.0",
    "xhr": "^2.4.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "~7.19.1",
    "eslint": "~8.28.0",
    "eslint-plugin-react": "^7.31.11",
    "nodemon": "^1.17.1",
    "webpack-dev-middleware": "^6.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
